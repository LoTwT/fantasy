{
  "private": true,
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "build": "nuxi build",
    "dev": "nuxi dev",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "start": "node .output/server/index.mjs",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --write .",
    "postinstall": "nuxi prepare",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "prepare": "simple-git-hooks",
    "up": "taze latest -I"
  },
  "devDependencies": {
    "@ayingott/eslint-config": "^0.2.5",
    "@iconify-json/carbon": "^1.1.18",
    "@iconify-json/twemoji": "^1.1.11",
    "@nuxt/devtools": "^0.6.7",
    "@nuxtjs/color-mode": "^3.3.0",
    "@pinia/nuxt": "^0.4.11",
    "@unocss/eslint-config": "^0.53.5",
    "@unocss/nuxt": "^0.53.5",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vueuse/nuxt": "^10.2.1",
    "consola": "^3.2.3",
    "eslint": "^8.44.0",
    "lint-staged": "^13.2.3",
    "nuxt": "^3.6.2",
    "pinia": "^2.1.4",
    "prettier": "^3.0.0",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.11.2",
    "typescript": "^5.1.6",
    "vue-tsc": "^1.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@ayingott/eslint-config"
  }
}
